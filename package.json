{
  "name": "solidity-docgen",
  "version": "0.5.3",
  "description": "Solidity API documentation automatic generator.",
  "bin": {
    "solidity-docgen": "./dist/cli.js"
  },
  "files": [
    "dist",
    "templates"
  ],
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "coverage": "nyc npm test",
    "docs": "oz-docs -c docs",
    "docs:watch": "npm run docs watch",
    "test": "tsc && ava",
    "test:watch": "fgbg 'ava --watch' 'tsc --watch'",
    "prepare": "npm run build"
  },
  "dependencies": {
    "@oclif/command": "^1.6.0",
    "@oclif/config": "^1.15.1",
    "@oclif/errors": "^1.2.2",
    "@oclif/plugin-help": "^2.2.3",
    "fs-extra": "^9.0.0",
    "globby": "^11.0.0",
    "handlebars": "^4.7.6",
    "json5": "^2.1.3",
    "lodash": "^4.17.15",
    "micromatch": "^4.0.2",
    "minimatch": "^3.0.4",
    "semver": "^7.3.2",
    "solc": "^0.6.6"
  },
  "devDependencies": {
    "@types/fs-extra": "^8.1.0",
    "@types/json5": "0.0.30",
    "@types/lodash": "^4.14.150",
    "@types/micromatch": "^4.0.1",
    "@types/minimatch": "^3.0.3",
    "@types/node": "^10.17.21",
    "@types/semver": "^7.1.0",
    "ava": "^3.8.0",
    "code-style": "github:OpenZeppelin/code-style",
    "codecov": "^3.6.5",
    "fgbg": "^0.1.4",
    "nyc": "^15.0.1",
    "openzeppelin-docs-utils": "github:OpenZeppelin/docs-utils",
    "solc-0-5": "npm:solc@^0.5.14",
    "solc-0-4": "npm:solc@^0.4.24",
    "typescript": "^3.8.3"
  },
  "keywords": [
    "solidity",
    "api",
    "documentation",
    "automatic",
    "generator"
  ],
  "license": "MIT"
}
